<%= form_for :user, class: 'form-group' do |form| %>
  <%= render 'name_elements', user: @user, form: form %>
  <%= render 'email_elements', user: @user, form: form %>
  <%= render 'password_elements', user: @user, form: form %>
  <%= render 'password_confirmation_elements', user: @user, form: form %>

  <%= form.submit 'Submit', class: 'btn btn-default', id: 'submit' %>
<% end %>
